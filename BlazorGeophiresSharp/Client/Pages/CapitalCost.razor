@code {
    [Parameter] public CapitalAndOMCostParameters Parms { get; set; }

    private double? totalCapitalCost;
    private double? wellDrillingCompletionCapitalCost;
    private double wellDrillingCompletionCapitalCostAdjustmentFactor;
    private int wellDrillingCostCorrelation;
    private double? reservoirStimulationCapitalCost;
    private double reservoirStimulationCapitalCostAdjustmentFactor;
    private double? surfacePlantCapitalCost;
    private double surfacePlantCapitalCostAdjustmentFactor;
    private double? fieldGatheringSystemCapitalCost;
    private double fieldGatheringSystemCapitalCostAdjustmentFactor;
    private double? explorationCapitalCost;
    private double explorationCapitalCostAdjustmentFactor;
    private double? totalOMCost;
    private double? wellfieldOMCost;
    private double wellfieldOMCostAdjustmentFactor;
    private double? surfacePlantOMCost;
    private double surfacePlantOMCostAdjustmentFactor;
    private double? waterCost;
    private double waterCostAdjustmentFactor;
    private double? electricityRate;
    private double? heatRate;

    protected override void OnParametersSet()
    {
        totalCapitalCost = Parms.TotalCapitalCost;
        if (totalCapitalCost == -1) totalCapitalCost = null;
        wellDrillingCompletionCapitalCost = Parms.ccwellfixed;
        if (wellDrillingCompletionCapitalCost == -1) wellDrillingCompletionCapitalCost = null;
        wellDrillingCompletionCapitalCostAdjustmentFactor = Parms.ccwelladjfactor;
        wellDrillingCostCorrelation = Parms.wellcorrelation;
        reservoirStimulationCapitalCost = Parms.ccstimfixed;
        if (reservoirStimulationCapitalCost == -1) reservoirStimulationCapitalCost = null;
        reservoirStimulationCapitalCostAdjustmentFactor = Parms.ccstimadjfactor;
        surfacePlantCapitalCost = Parms.ccplantfixed;
        if (surfacePlantCapitalCost == -1) surfacePlantCapitalCost = null;
        surfacePlantCapitalCostAdjustmentFactor = Parms.ccplantadjfactor;
        fieldGatheringSystemCapitalCost = Parms.ccgathfixed;
        if (fieldGatheringSystemCapitalCost == -1) fieldGatheringSystemCapitalCost = null;
        fieldGatheringSystemCapitalCostAdjustmentFactor = Parms.ccgathadjfactor;
        explorationCapitalCost = Parms.ccexplfixed;
        if (explorationCapitalCost == -1) explorationCapitalCost = null;
        explorationCapitalCostAdjustmentFactor = Parms.ccexpladjfactor;
        totalOMCost = Parms.oamtotalfixed;
        if (totalOMCost == -1) totalOMCost = null;
        wellfieldOMCost = Parms.oamwellfixed;
        if (wellfieldOMCost == -1) wellfieldOMCost = null;
        wellfieldOMCostAdjustmentFactor = Parms.oamwelladjfactor;
        surfacePlantOMCost = Parms.oamplantfixed;
        if (surfacePlantOMCost == -1) surfacePlantOMCost = null;
        surfacePlantOMCostAdjustmentFactor = Parms.oamplantadjfactor;
        waterCost = Parms.oamwaterfixed;
        if (waterCost == -1) waterCost = null;
        waterCostAdjustmentFactor = Parms.oamwateradjfactor;
        electricityRate = Parms.elecprice;
        if (electricityRate == -1) electricityRate = null;
        heatRate = Parms.heatprice;
        if (heatRate == -1) heatRate = null;
    }
}
